{
    "name": "tonybogdanov/tb-framework",
    "description": "A versatile WordPress framework for theme and plugin development",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Tony Bogdanov",
            "email": "support@tonybogdanov.com"
        }
    ],
    "config": {
        "vendor-dir": "tb-framework/vendor/"
    },
    "autoload": {
        "psr-4": {
            "TB\\Framework\\": "tb-framework/classes"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "3.4.x-dev",
        "symfony/debug": "3.4.x-dev"
    },
    "require": {
        "symfony/dependency-injection": "3.4.x-dev",
        "behat/transliterator": "^1.2@dev",
        "symfony/config": "3.4.x-dev",
        "symfony/yaml": "3.4.x-dev",
        "wa72/htmlpagedom": "dev-master",
        "guzzlehttp/guzzle": "^6.2@dev",
        "symfony/cache": "3.4.x-dev"
    }
}
